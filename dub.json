{
  "name" : "oauth",
  "description" : "OAuth2 client library compatible with vibe.d",
  "copyright" : "Copyright Â© 2016,2017 Harry T. Vennik",
  "license" : "MIT",
  "authors" : [
    "Harry T. Vennik",
    "Sebastian Wilzbach"
  ],
  "targetType" : "library",
  "dependencies" : {
    "vibe-d:core" : "~>0.8.0-beta.6",
    "vibe-d:data" : "*",
    "vibe-d:http" : "*",
    "vibe-d:web" : {
      "version" : "*",
      "optional" : true
    }
  },
  "configurations": [
      {
          "name": "library",
          "excludedSourceFiles": ["source/oauth/test.d"]
      },
      {
          "name": "unittest",
          "targetType": "executable",
          "preBuildCommands": ["dub run unit-threaded -c gen_ut_main -- -f ut.d"],
          "postBuildCommands": ["rm ut.d"],
          "mainSourceFile": "ut.d",
          "dependencies": {
              "unit-threaded": "~>0.7.11"
          }
      }
  ],
  "-ddoxTool": "scod"
}
